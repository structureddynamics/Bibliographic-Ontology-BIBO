@prefix : <http://purl.org/ontology/bibo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/ontology/bibo/> .

<http://purl.org/ontology/bibo/> rdf:type owl:Ontology ;
                                  <http://purl.org/dc/terms/creator> :bdarcus ,
                                                                     :fgiasson ;
                                  <http://purl.org/dc/terms/description> """The Bibliographic Ontology describes
bibliographic things on the semantic Web in RDF.  This ontology can be
used as a citation ontology, as a document classification ontology, or
simply as a way to describe any kind of document in RDF. It has been
inspired by many existing document description metadata formats, and
can be used as a common ground for converting other bibliographic data
sources."""@en ;
                                  <http://purl.org/dc/terms/title> "The Bibliographic Ontology"^^xsd:string ;
                                  owl:versionInfo "http://purl.org/ontology/bibo/1.3/"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/identifier
<http://purl.org/dc/terms/identifier> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#deprecated
owl:deprecated rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/06/sw-vocab-status/ns#term_status
<http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#changeNote
<http://www.w3.org/2004/02/skos/core#changeNote> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#editorialNote
<http://www.w3.org/2004/02/skos/core#editorialNote> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#historyNote
<http://www.w3.org/2004/02/skos/core#historyNote> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
<http://www.w3.org/2004/02/skos/core#note> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#scopeNote
<http://www.w3.org/2004/02/skos/core#scopeNote> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/NET/c4dm/event.owl#agent
<http://purl.org/NET/c4dm/event.owl#agent> rdf:type owl:ObjectProperty ;
                                           <http://www.w3.org/2004/02/skos/core#changeNote> "Used to link an agent (a person) to an event (a conference, an hearing, etc.)"@en .


###  http://purl.org/NET/c4dm/event.owl#place
<http://purl.org/NET/c4dm/event.owl#place> rdf:type owl:ObjectProperty ;
                                           <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to relate an event such as a conference to the geographical place where it happens, for example Paris."@en .


###  http://purl.org/NET/c4dm/event.owl#produced_in
<http://purl.org/NET/c4dm/event.owl#produced_in> rdf:type owl:ObjectProperty .


###  http://purl.org/NET/c4dm/event.owl#product
<http://purl.org/NET/c4dm/event.owl#product> rdf:type owl:ObjectProperty ;
                                             <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to link an event such as a conference to an outcome (a product) of that event, for example, an article, a proceeding, etc."@en .


###  http://purl.org/NET/c4dm/event.owl#sub_event
<http://purl.org/NET/c4dm/event.owl#sub_event> rdf:type owl:ObjectProperty ;
                                               <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to link big events with smaller events such as workshops that happen in the context of a conference."@en .


###  http://purl.org/NET/c4dm/event.owl#time
<http://purl.org/NET/c4dm/event.owl#time> rdf:type owl:ObjectProperty ;
                                          <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to describe the timing of an event. For example, when a conference starts and stops."@en .


###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:ObjectProperty ;
                                       <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to link a bibliographic item to one of its contributor: can be an author, an editor, a publisher, etc."@en .


###  http://purl.org/dc/terms/format
<http://purl.org/dc/terms/format> rdf:type owl:ObjectProperty ;
                                  <http://www.w3.org/2004/02/skos/core#example> """<dcterms:format>
   <dcterms:MediaTypeOrExtent>
     <rdf:value>text/html</rdf:value>
     <rdfs:label>HTML</rdfs:label>
   </dcterms:MediaTypeOrExtent>
 </dcterms:format>"""@en ;
                                  <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to describe the format of a bibliographic resource."@en .


###  http://purl.org/dc/terms/hasPart
<http://purl.org/dc/terms/hasPart> rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/isPartOf
<http://purl.org/dc/terms/isPartOf> rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/isReferencedBy
<http://purl.org/dc/terms/isReferencedBy> rdf:type owl:ObjectProperty ;
                                          <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to relate a reference citation to a bibliographic resource."@en .


###  http://purl.org/dc/terms/isVersionOf
<http://purl.org/dc/terms/isVersionOf> rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/language
<http://purl.org/dc/terms/language> rdf:type owl:ObjectProperty ;
                                    <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to link a bibliographic resource to the language used to express it."@en .


###  http://purl.org/dc/terms/publisher
<http://purl.org/dc/terms/publisher> rdf:type owl:ObjectProperty ;
                                     <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to link a bibliographic item to its publisher."@en .


###  http://purl.org/dc/terms/references
<http://purl.org/dc/terms/references> rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/relation
<http://purl.org/dc/terms/relation> rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/rights
<http://purl.org/dc/terms/rights> rdf:type owl:ObjectProperty ;
                                  <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to describe rights related to a bibliographic resource."@en .


###  http://purl.org/dc/terms/subject
<http://purl.org/dc/terms/subject> rdf:type owl:ObjectProperty ;
                                   <http://www.w3.org/2004/02/skos/core#scopeNote> "Used to describe the subject of a bibliographic resource."@en .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:ObjectProperty .


###  http://purl.org/ontology/bibo/affirmedBy
:affirmedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :subsequentLegalDecision ;
            rdfs:domain :LegalDecision ;
            rdfs:range :LegalDecision ;
            rdfs:comment "A legal decision that affirms a ruling."@en ;
            rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI .


###  http://purl.org/ontology/bibo/annotates
:annotates rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://purl.org/dc/terms/relation> ;
           rdfs:domain :Note ;
           rdfs:range rdfs:Resource ;
           rdfs:comment "Critical or explanatory note for a Document."@en ;
           rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
           rdfs:label "annotates"@en ;
           <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/authorList
:authorList rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :contributorList ;
            rdfs:domain :Document ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( rdf:List
                                       rdf:Seq
                                     )
                       ] ;
            rdfs:comment "An ordered list of authors. Normally, this list is seen as a priority list that order authors by importance."@en ;
            rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
            rdfs:label "list of authors"@en ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/citedBy
:citedBy rdf:type owl:ObjectProperty ;
         owl:inverseOf :cites ;
         rdfs:domain :Document ;
         rdfs:range :Document ;
         rdfs:comment """Relates a document to another document that cites the
first document."""@en ;
         rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
         rdfs:label "cited by"@en ;
         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string .


###  http://purl.org/ontology/bibo/cites
:cites rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf <http://purl.org/dc/terms/references> ;
       rdfs:domain :Document ;
       rdfs:range :Document ;
       rdfs:comment """Relates a document to another document that is cited
by the first document as reference, comment, review, quotation or for
another purpose."""@en ;
       rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
       rdfs:label "cites"@en ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string .


###  http://purl.org/ontology/bibo/contributorList
:contributorList rdf:type owl:ObjectProperty ;
                 rdfs:domain :Document ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( rdf:List
                                            rdf:Seq
                                          )
                            ] ;
                 rdfs:comment "An ordered list of contributors. Normally, this list is seen as a priority list that order contributors by importance."@en ;
                 rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
                 rdfs:label "list of contributors"@en ;
                 <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/court
:court rdf:type owl:ObjectProperty ;
       rdfs:domain :LegalDocument ;
       rdfs:range <http://xmlns.com/foaf/0.1/Organization> ;
       rdfs:comment "A court associated with a legal document; for example, that which issues a decision."@en ;
       rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
       rdfs:label "court"@en ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string .


###  http://purl.org/ontology/bibo/degree
:degree rdf:type owl:ObjectProperty ;
        rdfs:domain :Thesis ;
        rdfs:range :ThesisDegree ;
        rdfs:comment "The thesis degree."@en ;
        rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
        rdfs:label "degree"@en ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string ;
        <http://www.w3.org/2004/02/skos/core#editorialNote> "We are not defining, using an enumeration, the range of the bibo:degree to the defined list of bibo:ThesisDegree. We won't do it because we want people to be able to define new degress if needed by some special usecases. Creating such an enumeration would restrict this to happen."@en .


###  http://purl.org/ontology/bibo/director
:director rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://purl.org/dc/terms/contributor> ;
          rdfs:domain :AudioVisualDocument ;
          rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
          rdfs:comment "A Film director."@en ;
          rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
          rdfs:label "director"^^xsd:string ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/distributor
:distributor rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Collection
                                         :Document
                                       )
                         ] ;
             rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
             rdfs:comment "Distributor of a document or a collection of documents."@en ;
             rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
             rdfs:label "distributor"@en ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/editor
:editor rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://purl.org/dc/terms/contributor> ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Collection
                                    :Document
                                  )
                    ] ;
        rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
        rdfs:comment "A person having managerial and sometimes policy-making responsibility for the editorial part of a publishing firm or of a newspaper, magazine, or other publication."@en ;
        rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
        rdfs:label "editor"^^xsd:string ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/editorList
:editorList rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :contributorList ;
            rdfs:domain :Document ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( rdf:List
                                       rdf:Seq
                                     )
                       ] ;
            rdfs:comment "An ordered list of editors. Normally, this list is seen as a priority list that order editors by importance."@en ;
            rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
            rdfs:label "list of editors"@en ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/interviewee
:interviewee rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/dc/terms/contributor> ;
             rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
             rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
             rdfs:comment "An agent that is interviewed by another agent."@en ;
             rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
             rdfs:label "interviewee"^^xsd:string ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/interviewer
:interviewer rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/dc/terms/contributor> ;
             rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
             rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
             rdfs:comment "An agent that interview another agent."@en ;
             rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
             rdfs:label "interviewer"^^xsd:string ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/issuer
:issuer rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://purl.org/dc/terms/publisher> ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Collection
                                    :Document
                                  )
                    ] ;
        rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
        rdfs:comment "An entity responsible for issuing often informally published documents such as press releases, reports, etc."^^xsd:string ;
        rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
        rdfs:label "issuer"^^xsd:string ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string .


###  http://purl.org/ontology/bibo/organizer
:organizer rdf:type owl:ObjectProperty ;
           rdfs:domain <http://purl.org/NET/c4dm/event.owl#Event> ;
           rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
           rdfs:comment "The organizer of an event; includes conference organizers, but also government agencies or other bodies that are responsible for conducting hearings."@en ;
           rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
           rdfs:label "organizer"@en ;
           <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string .


###  http://purl.org/ontology/bibo/owner
:owner rdf:type owl:ObjectProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Collection
                                   :Document
                                 )
                   ] ;
       rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
       rdfs:comment "Owner of a document or a collection of documents."@en ;
       rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
       rdfs:label "owner"@en ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string .


###  http://purl.org/ontology/bibo/performer
:performer rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://purl.org/dc/terms/contributor> ;
           rdfs:domain :Performance ;
           rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
           rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
           rdfs:label "performer"^^xsd:string ;
           <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/presentedAt
:presentedAt rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/NET/c4dm/event.owl#produced_in> ;
             rdfs:domain :Document ;
             rdfs:range :Event ;
             rdfs:comment "Relates a document to an event; for example, a paper to a conference."@en ;
             rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
             rdfs:label "presented at"@en ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string .


###  http://purl.org/ontology/bibo/presents
:presents rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://purl.org/NET/c4dm/event.owl#product> ;
          owl:inverseOf :presentedAt ;
          rdfs:domain :Event ;
          rdfs:range :Document ;
          rdfs:comment "Relates an event to associated documents; for example, conference to a paper."@en ;
          rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
          rdfs:label "presents"@en ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string .


###  http://purl.org/ontology/bibo/producer
:producer rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Collection
                                      :Document
                                    )
                      ] ;
          rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
          rdfs:comment "Producer of a document or a collection of documents."@en ;
          rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
          rdfs:label "producer"@en ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/recipient
:recipient rdf:type owl:ObjectProperty ;
           rdfs:domain :PersonalCommunicationDocument ;
           rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
           rdfs:comment "An agent that receives a communication document."@en ;
           rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
           rdfs:label "recipient"^^xsd:string ;
           <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/reproducedIn
:reproducedIn rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.org/dc/terms/isPartOf> ;
              rdfs:domain :Document ;
              rdfs:range :Document ;
              rdfs:comment "The resource in which another resource is reproduced."@en ;
              rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
              <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable"^^xsd:string .


###  http://purl.org/ontology/bibo/reversedBy
:reversedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :subsequentLegalDecision ;
            rdfs:domain :LegalDecision ;
            rdfs:range :LegalDecision ;
            rdfs:comment "A legal decision that reverses a ruling."@en ;
            rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI .


###  http://purl.org/ontology/bibo/reviewOf
:reviewOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://purl.org/dc/terms/relation> ;
          rdfs:domain :Document ;
          rdfs:range rdfs:Resource ;
          rdfs:comment "Relates a review document to a reviewed thing (resource, item, etc.)."@en ;
          rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
          rdfs:label "review of"@en ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string .


###  http://purl.org/ontology/bibo/status
:status rdf:type owl:ObjectProperty ;
        rdfs:domain :Document ;
        rdfs:range :DocumentStatus ;
        rdfs:comment "The publication status of (typically academic) content."@en ;
        rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
        rdfs:label "status"@en ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable"^^xsd:string ;
        <http://www.w3.org/2004/02/skos/core#editorialNote> "We are not defining, using an enumeration, the range of the bibo:status to the defined list of bibo:DocumentStatus. We won't do it because we want people to be able to define new status if needed by some special usecases. Creating such an enumeration would restrict this to happen."@en .


###  http://purl.org/ontology/bibo/subsequentLegalDecision
:subsequentLegalDecision rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <http://purl.org/dc/terms/isReferencedBy> ;
                         rdfs:domain :LegalDecision ;
                         rdfs:range :LegalDecision ;
                         rdfs:comment "A legal decision on appeal that takes action on a case (affirming it, reversing it, etc.)."@en ;
                         rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI .


###  http://purl.org/ontology/bibo/transcriptOf
:transcriptOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.org/dc/terms/relation> ;
              rdfs:domain :Document ;
              rdfs:range rdfs:Resource ;
              rdfs:comment "Relates a document to some transcribed original."@en ;
              rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
              rdfs:label "transcript of"@en ;
